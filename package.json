{
  "name": "minimessageboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/netlify-lambda serve src",
    "build": "NODE_ENV=production ./node_modules/.bin/netlify-lambda build src",
    "devstart": "npm run watch:css && nodemon ./bin/www",
    "watch:css": "postcss public/style/tailwind.css -o dist/output.css"
  },
  "tailwind:css": "postcss public/style/tailwind.css -o public/style/mystyle.css",
  "dependencies": {
    "async": "^3.2.4",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.8",
    "express": "~4.16.1",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "luxon": "^3.2.1",
    "morgan": "~1.9.1",
    "netlify-lambda": "^2.0.15",
    "nodemon": "^2.0.20",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.3",
    "serverless-http": "^3.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
}
